    priority = {0},
    title = {Complement Factor {H} Polymorphism in Age-Related Macular Degeneration},
    url = {http://dx.doi.org/10.1126/science.1109557},
    citeulike-article-id = {215714},
    citeulike-linkout-0 = {http://dx.doi.org/10.1126/science.1109557},
    citeulike-linkout-1 = {http://www.sciencemag.org/cgi/content/abstract/308/5720/385},
