  keywords = {First-digit law, Benford's law, significant-digit law, scale invariance, base invariance, random distributions, random probability measures, random $k$-samples, mantissa, logarithmic law, mantissa sigma algebra},
  month = nov,
  number = {4},
  pages = {354--363},
  publisher = {Institute of Mathematical Statistics},
  title = {A Statistical Derivation of the Significant-Digit Law},
  url = {http://links.jstor.org/sici?sici=0883-4237%28199511%2910%3A4%3C354%3AASDOTS%3E2.0.CO%3B2-P},
